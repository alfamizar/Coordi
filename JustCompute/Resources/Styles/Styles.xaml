<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Style TargetType="ActivityIndicator">
        <Setter Property="Color"
                Value="{AppThemeBinding Light={StaticResource Primary}, Dark={StaticResource White}}" />
    </Style>

    <Style TargetType="IndicatorView">
        <Setter Property="IndicatorColor"
                Value="{AppThemeBinding Light={StaticResource Gray200}, Dark={StaticResource Gray500}}" />
        <Setter Property="SelectedIndicatorColor"
                Value="{AppThemeBinding Light={StaticResource Gray950}, Dark={StaticResource Gray100}}" />
    </Style>

    <Style TargetType="Border">
        <Setter Property="Stroke"
                Value="{AppThemeBinding Light={StaticResource Gray200}, Dark={StaticResource Gray500}}" />
        <Setter Property="StrokeShape"
                Value="RoundRectangle 10" />
        <Setter Property="StrokeThickness"
                Value="1" />
        <Setter Property="Background"
                Value="{AppThemeBinding Light={StaticResource LightCollectionViewItemBackgroundColor}, Dark={StaticResource DarkCollectionViewItemBackgroundColor}}" />
        <Setter Property="Padding"
                Value="16, 8" />
        <Setter Property="Margin"
                Value="8, 4" />
        <Setter Property="Shadow">
            <Setter.Value>
                <Shadow Brush="{AppThemeBinding Light={StaticResource Black}, Dark={StaticResource White}}"
                        Opacity="0.3"
                        Radius="10"
                        Offset="2,2" />
            </Setter.Value>
        </Setter>

    </Style>

    <Style TargetType="BoxView">
        <Setter Property="Color"
                Value="{AppThemeBinding Light={StaticResource Gray950}, Dark={StaticResource Gray200}}" />
    </Style>

    <Style TargetType="BoxView"
           x:Key="DividerStyle">
        <Setter Property="HeightRequest"
                Value="2" />
        <Setter Property="Color"
                Value="{AppThemeBinding Light={StaticResource LightLinesDividerColor}, Dark={StaticResource DarkLinesDividerColor}}" />
    </Style>

    <Style TargetType="Grid"
           x:Key="GridDialogContent">
        <Setter Property="BackgroundColor"
                Value="{AppThemeBinding Light={StaticResource LightPageBackgroundColor}, Dark={StaticResource DarkPageBackgroundColor}}" />
        <Setter Property="RowSpacing"
                Value="8" />
        <Setter Property="ColumnSpacing"
                Value="8" />
        <Setter Property="Padding"
                Value="8" />
    </Style>

    <Style TargetType="Button"
           x:Key="DialogButton">
        <Setter Property="Opacity"
                Value="1" />
        <Setter Property="BackgroundColor"
                Value="{AppThemeBinding Light={StaticResource LightButtonBackgroundColor}, 
                                        Dark={StaticResource DarkButtonBackgroundColor}}" />
        <Setter Property="TextColor"
                Value="{AppThemeBinding Light={StaticResource LightLabelTextColorColor}, 
                                        Dark={StaticResource DarkLabelTextColorColor}}" />
        <Setter Property="BorderColor"
                Value="Gray" />
        <Setter Property="BorderWidth"
                Value="1" />
        <Setter Property="CornerRadius"
                Value="5" />
        <Setter Property="MinimumHeightRequest"
                Value="44" />
        <Setter Property="MinimumWidthRequest"
                Value="44" />
        <Setter Property="VisualStateManager.VisualStateGroups">
            <VisualStateGroupList>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <VisualState.Setters>
                            <Setter Property="Scale"
                                    Value="1" />
                        </VisualState.Setters>
                    </VisualState>
                    <VisualState x:Name="Enabled">
                        <VisualState.Setters>
                            <Setter Property="Scale"
                                    Value="1" />
                        </VisualState.Setters>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <VisualState.Setters>
                            <Setter Property="Scale"
                                    Value="0.8" />
                        </VisualState.Setters>
                    </VisualState>
                    <VisualState x:Name="PointerOver" />
                </VisualStateGroup>
            </VisualStateGroupList>
        </Setter>
    </Style>

    <Style TargetType="Button">
        <Setter Property="TextColor"
                Value="{AppThemeBinding Light={StaticResource LightLabelTextColorColor}, 
                                        Dark={StaticResource DarkLabelTextColorColor}}" />
        <Setter Property="BackgroundColor"
